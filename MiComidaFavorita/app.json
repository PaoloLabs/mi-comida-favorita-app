{
  "expo": {
    "name": "MiComidaFavorita",
    "slug": "MiComidaFavorita",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photoCameraPermission": "La aplicación necesita acceso a la cámara para tomar fotos de perfil.",
          "mediaLibraryPermission": "La aplicación necesita acceso a tus fotos para seleccionar una imagen de perfil."
        }
      ]
    ]
  }
}
